{
    "name": "tgozo/laravel-codegen",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tgozo\\LaravelCodegen\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Takudzwa Gozo",
            "email": "thetakudzwagozo@gmail.com"
        }
    ],
    "require": {
        "doctrine/inflector": "2.1.x-dev"
    },
    "require-dev": {
        "pestphp/pest": "^2.4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tgozo\\LaravelCodegen\\CodeGenServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
