{
    "name": "tgozo/laravel-codegen",
    "description": "A Laravel Code Generator to help you get started with a lot of staff including generating Migrations, Models, Controllers, Factories, Routes, Views, Database Seeders and Request Validation Rules.",
    "keywords": [
        "php",
        "framework",
        "laravel",
        "codegen",
        "code",
        "generator"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tgozo\\LaravelCodegen\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Takudzwa Gozo",
            "email": "thetakudzwagozo@gmail.com"
        }
    ],
    "require": {
        "doctrine/inflector": "2.1.x-dev"
    },
    "require-dev": {
        "pestphp/pest": "^2.4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tgozo\\LaravelCodegen\\CodeGenServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
